{
  "name": "co2-box-configuration-page",
  "version": "1.0.0",
  "main": "src/index.tsx",
  "homepage": ".",
  "private": true,
  "config-overrides-path": "config-overrides.cjs",
  "dependencies": {
    "@amcharts/amcharts5": "^5.3.7",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.1",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "pondjs": "^0.8.8",
    "postman-to-openapi": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-timeseries-charts": "^0.16.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/react-dom": "17.0.9",
    "cli-glob": "0.1.0",
    "clsx": "^1.1.1",
    "customize-cra": "1.0.0",
    "glob": "^8.0.3",
    "http-proxy-middleware": "^2.0.6",
    "jest-sonar-reporter": "^2.0.0",
    "node-sass": "6.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "4.0.3",
    "ts-jest": "^26.5.6",
    "typescript": "4.4.4"
  },
  "scripts": {
    "build:js": "react-scripts build",
    "build": "npm-run-all build:js",
    "prettier": "prettier --write \"**/*.ts\" \"**/*.json\" \"**/*.tsx\" \"**/*.js\" \"**/*.css\" \"**/*.scss\"",
    "sonar": "react-scripts test --coverage --watchAll false --testResultsProcessor jest-sonar-reporter",
    "start": "react-app-rewired start",
    "test": "react-scripts test --clearCache && react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest": {
    "resetMocks": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      ".+\\.(css|less|sass|scss|png|jpg)$": "<rootDir>/fileTransformer.cjs"
    },
    "transformIgnorePatterns": [
      "node_modules/@mui/material|@mui/system|@babel)/)",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
  },
  "packageManager": "yarn@3.2.4"
}
